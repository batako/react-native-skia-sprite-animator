{
  "name": "standalone-editor",
  "version": "0.1.0",
  "private": true,
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "expo start --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "generate:licenses": "node scripts/generate-licenses.cjs",
    "expo:login": "npx eas login",
    "expo:configure": "npx eas build:configure --platform ios",
    "expo:prebuild:ios": "npx expo prebuild --platform ios",
    "expo:build:ios": "npx eas build --platform ios --profile production",
    "expo:submit:ios": "npx eas submit --platform ios --profile production",
    "expo:release:ios": "npm run expo:build:ios && npm run expo:submit:ios"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@shopify/react-native-skia": "2.2.12",
    "expo": "~54.0.25",
    "expo-asset": "~12.0.10",
    "expo-document-picker": "~14.0.7",
    "expo-file-system": "~19.0.19",
    "expo-font": "~14.0.9",
    "expo-image-manipulator": "~14.0.7",
    "expo-localization": "~17.0.7",
    "expo-sharing": "~14.0.7",
    "expo-updates": "~29.0.13",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.3",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-skia-sprite-animator": "file:../../",
    "react-native-svg": "15.12.1",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "react-native-worklets-core": "^1.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@tsconfig/react-native": "^3.0.5",
    "@types/react": "~19.1.0",
    "license-checker-rseidelsohn": "^4.4.2",
    "typescript": "~5.9.2"
  }
}
